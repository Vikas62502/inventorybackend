{
  "name": "chairbord-solar-inventory-backend",
  "version": "1.0.0",
  "description": "Backend API for Chairbord Solar Inventory Management System",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "nodemon --exec ts-node server.ts",
    "type-check": "tsc --noEmit",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "init:superadmin": "ts-node scripts/initSuperAdmin.ts",
    "init:quotation": "ts-node scripts/initQuotationAdmin.ts",
    "check:admin": "ts-node scripts/checkAndFixAdmin.ts"
  },
  "keywords": [
    "inventory",
    "management",
    "solar"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.1693.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "mime-types": "^3.0.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.3",
    "sequelize": "6.35.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^9.0.1",
    "winston": "^3.18.3",
    "winston-loki": "^6.1.3",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/mime-types": "^3.0.1",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.6",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^9.0.7",
    "@types/winston": "^2.4.4",
    "nodemon": "^3.0.2",
    "sequelize-cli": "^6.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
